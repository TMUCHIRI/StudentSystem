import java.sql.*;

public class Student_system {

    public static void main(String[] args) {
       try{
        Class.forName(  "org.mariadb.jdbc.Driver");
        Connection conObj = DriverManager.getConnection("jdbc:mariadb://localhost:3306/task2","root","39971939");
        Statement statemt = conObj.createStatement();
        ResultSet rst  = statemt.executeQuery("SHOW TABLES;");
        while(rst.next()){
            System.out.println(rst.getString(1));
        }
        conObj.close();
    }
       catch(Exception e){
           System.out.println(e);         
       }
    }
}
